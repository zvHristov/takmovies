{
  "expo": {
    "name": "TakMovies",
    "slug": "takmovies",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "takmovies",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "buildNumber": "1",
      "supportsTablet": true,
      "bundleIdentifier": "com.kingkongruderuders.takmovies",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      
      "package": "com.kingkongruderuders.takmovies",
      "googleServicesFile": "./google-services.json" 
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      // "@react-native-firebase/auth",
      "@react-native-firebase/crashlytics",
      "@react-native-firebase/app",
      "@react-native-google-signin/google-signin",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0"
          },
          "ios": {
            "deploymentTarget": "13.4"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "91ad690f-df13-4d71-b926-8477c40746ab"
      }
    },
    "owner": "kingkong_rude_ruders",
    "updates": {
      "url": "https://u.expo.dev/91ad690f-df13-4d71-b926-8477c40746ab"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "jsEngine": "hermes"
  }
}
