{
  "expo": {
    "name": "TakMovies",
    "slug": "takmovies",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "takmovies",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kingkongruderuders.takmovies"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kingkongruderuders.takmovies"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "@react-native-google-signin/google-signin",
        "@react-native-firebase/app"
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "91ad690f-df13-4d71-b926-8477c40746ab"
      }
    },
    "owner": "kingkong_rude_ruders",
    "updates": {
      "url": "https://u.expo.dev/91ad690f-df13-4d71-b926-8477c40746ab"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "jsEngine": "hermes"
  }
}
